{
  "name": "tph-website",
  "version": "0.1.0",
  "private": true,
  "description": "The official website for The Programmers Hangout",
  "keywords": [
    "gatsby",
    "TPH",
    "The Programmers Hangout"
  ],
  "bugs": {
    "url": "https://github.com/the-programmers-hangout/website/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/the-programmers-hangout/website"
  },
  "license": "MIT",
  "author": "TPH",
  "scripts": {
    "build": "gatsby build",
    "codegen": "graphql-codegen --config codegen.yml",
    "develop": "gatsby develop",
    "fetch-users": "node ./scripts/fetchUsers.js",
    "format": "prettier --write src/{**,**/**}/*.{js,ts,tsx,md}",
    "lint": "eslint '{src,scripts}/**/*.{js,ts,tsx}'",
    "lint:fix": "yarn run lint -- --fix",
    "serve": "gatsby serve",
    "start": "yarn run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@el7cosmos/gatsby-plugin-prefetch-google-fonts": "^2.0.3",
    "@mdx-js/mdx": "^2.1.5",
    "@mdx-js/react": "^2.1.5",
    "@reach/router": "^1.3.4",
    "classnames": "^2.3.2",
    "gatsby": "^4.24.5",
    "gatsby-plugin-layout": "^3.24.0",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-mdx": "^4.3.0",
    "gatsby-plugin-polished": "^1.0.4",
    "gatsby-plugin-postcss": "^5.24.0",
    "gatsby-plugin-react-helmet": "^5.24.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-sharp": "^4.24.0",
    "gatsby-plugin-styled-components": "^5.24.0",
    "gatsby-remark-auto-headers-improved": "^1.0.4",
    "gatsby-source-filesystem": "^4.24.0",
    "gatsby-transformer-sharp": "^4.24.0",
    "github-slugger": "^1.5.0",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "polished": "^4.2.2",
    "postcss": "^8.4.18",
    "prismjs": "^1.29.0",
    "ramda": "^0.28.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-highlight": "^0.14.0",
    "react-is": "^18.2.0",
    "react-particles-js": "^3.5.3",
    "react-perfect-scrollbar": "^1.5.8",
    "react-scrollspy": "^3.4.3",
    "react-use": "^17.4.0",
    "styled-components": "^5.3.6",
    "tsparticles": "^2.3.4",
    "typescript": "^4.8.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.8",
    "@graphql-codegen/typescript": "2.8.0",
    "@graphql-codegen/typescript-operations": "2.5.5",
    "@graphql-codegen/typescript-react-apollo": "3.3.5",
    "@types/classnames": "^2.3.0",
    "@types/lodash": "^4.14.186",
    "@types/prismjs": "^1.16.5",
    "@types/ramda": "^0.27.40",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-helmet": "^6.1.1",
    "@types/react-scrollspy": "^3.3.3",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.26.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.13.2",
    "discord.js": "^13.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-remove-trailing-slashes": "^3.14.0",
    "prettier": "^2.4.1",
    "svg-react-loader": "^0.4.6",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
